{
  "name": "@sumologic/opentelemetry-rum",
  "version": "2.0.1",
  "description": "Adds OpenTelemetry tracing auto-instrumentation in the browser. Collects spans on network events and sends them to Sumo Logic.",
  "main": "./dist/index.js",
  "browser": "./dist/browser.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "node ./scripts/bundle.js && rollup -c",
    "prettier-check-all": "prettier --check .",
    "prettier-format-all": "prettier --write .",
    "test": "npx jest && npx playwright test e2e_test/ && npm run prettier-check-all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "repository": "SumoLogic/sumologic-opentelemetry-js",
  "author": "Sumo Logic Authors",
  "license": "Apache-2.0",
  "keywords": [
    "opentelemetry",
    "tracing",
    "sumologic",
    "sumo",
    "rum",
    "ot"
  ],
  "devDependencies": {
    "@playwright/test": "^1.15.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.2",
    "@types/shimmer": "^1.0.2",
    "axios": "^0.21.4",
    "express": "^4.17.1",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "lodash.merge": "^4.6.2",
    "playwright": "^1.15.0",
    "prettier": "^2.4.1",
    "rollup": "^2.57.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "shimmer": "^1.2.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3",
    "zone.js": "^0.11.4"
  },
  "peerDependencies": {
    "@opentelemetry/api": "1.0.3",
    "@opentelemetry/tracing": "0.24.0"
  }
}
